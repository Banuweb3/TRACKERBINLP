{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "server/simple-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/simple-server.js"
    },
    {
      "src": "/health",
      "dest": "/server/simple-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DB_HOST": "trackerbi-do-user-17425890-0.m.db.ondigitalocean.com",
    "DB_PORT": "25060",
    "DB_USER": "doadmin",
    "DB_NAME": "defaultdb",
    "DB_SSLMODE": "REQUIRED"
  }
}
